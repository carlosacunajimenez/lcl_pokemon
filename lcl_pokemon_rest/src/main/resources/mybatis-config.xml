<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- Example configuration, adjust as per your setup -->
    <typeAliases>
        <typeAlias alias="pokemonType" type="lcl.pokemon.domain.PokemonType"/>
        <typeAlias alias="pokemon" type="lcl.pokemon.domain.Pokemon"/>
    </typeAliases>

    <!-- Add mappers -->
    <mappers>
        <mapper resource="mappers/PokemonTypeMapper.xml"/>
        <mapper resource="mappers/PokemonMapper.xml"/>
        <!-- Add more mappers as needed -->
    </mappers>
</configuration>
